<?xml version="1.0" encoding="utf-8"?><!-- JNLP File for PictureBrowserServlet client application -->
<jnlp spec="1.0+"
      codebase="http://ivy-tools.googlecode.com/svn/branches/gradle/webstart/"
      href="ivyBuilder.jnlp">
    <information>
        <title>IvyBuilder</title>
        <vendor>Nurflugel Software</vendor>
        <homepage href="index.html"/>
        <description>Ivy Repository Builder</description>
        <icon href="images/IvyBuilder-small.png"/>
        <icon kind="splash"
              href="images/IvyBuilder-splash.jpg"/>
        <offline-allowed/>
        <shortcut online="true">
           <desktop/>
         </shortcut>
    </information>
    <offline-allowed/>
    <security>
        <all-permissions/>
    </security>
    <update check="timeout"
            policy="always"/>
    <resources>
        <j2se version="1.6+"
              initial-heap-size="64m"
              max-heap-size="512m"/>
        <jar href="lib/IvyTools.jar"/>
        <jar href="lib/commons-lang-2.4.jar"/>
        <jar href="lib/jdom-1.0.jar"/>
        <jar href="lib/glazedlists_java15-1.8.0.jar"/>
      <!--<jar href="lib/help.jar"/>-->
      <extension name="help"
                 href="help.jnlp"/>
    </resources>
    <application-desc main-class="com.nurflugel.ivybuilder.BuilderMainFrame"/>
</jnlp>

